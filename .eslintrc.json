{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-unused-vars": "warn",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off"
    }
}
